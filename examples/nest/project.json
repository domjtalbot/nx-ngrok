{
  "name": "examples-nest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/nest/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/examples/nest",
        "main": "examples/nest/src/main.ts",
        "tsConfig": "examples/nest/tsconfig.app.json",
        "assets": ["examples/nest/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "examples/nest/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "examples-nest:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "examples-nest:build:production"
        }
      }
    },
    "dev": {
      "executor": "nx-ngrok:tunnel",
      "options": {
        "serverTarget": "examples-nest:serve:development",
        "address": 3333
      },
      "configurations": {
        "production": {
          "serverTarget": "examples-nest:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["examples/nest/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "examples/nest/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["type:example"]
}
